#%Module1.0
module-whatis "octave"
module-whatis "Version: VERSION"
module-whatis "Compiler: COMPILERNAME"
setenv OCTAVEHOME /opt/octave
prepend-path PATH /opt/octave/bin
prepend-path LD_LIBRARY_PATH /opt/octave/lib/octave/VERSION
prepend-path LIBPATH /opt/octave/lib/octave/VERSION
module load ROLLCOMPILER ROLLMPI mkl
